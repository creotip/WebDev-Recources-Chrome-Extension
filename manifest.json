{
  "manifest_version": 2,
  "name": "CodeNews - Web Design & Development Resources",
   "short_name": "CodeNews",
  "description": "This extension shows a list of best Recources for Designers and developers",
  "version": "1.0.1",
   "author": "creotip.io",
  

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
     "storage"
  ],
  "icons": { "16": "icon.png", "128": "webdesign.png" },
  
  "web_accessible_resources": [
    "images/*.png",
    "images/*.svg",
    "images/*.jpg"
  ]
}




